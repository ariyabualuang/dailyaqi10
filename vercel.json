{
    "version": 2,
    "builds": [
      {
        "src": "index.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],
     "crons": [
          {
            "path": "/index.js",
            "schedule": "45 11 * * *"
          }
    ]
      
  }